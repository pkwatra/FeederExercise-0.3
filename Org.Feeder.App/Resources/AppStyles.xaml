<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >

    <ImageSource x:Key="App.Background.ImageSource">./Images/background.jpeg</ImageSource>
    <ImageBrush x:Key="App.Background.Brush" ImageSource="{StaticResource App.Background.ImageSource}" />

    <!-- Text Styles -->
    <Style x:Key="Text.Base" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Segoe UI Light" />
    </Style>

    <Style x:Key="Text.H1" TargetType="TextBlock" BasedOn="{StaticResource Text.Base}">
        <Setter Property="FontSize" Value="24" />
    </Style>

    <Style x:Key="Text.H2" TargetType="TextBlock" BasedOn="{StaticResource Text.Base}">
        <Setter Property="FontSize" Value="20" />
    </Style>

    <Style x:Key="Text.Post.Header" TargetType="TextBlock" BasedOn="{StaticResource Text.H1}">
        <Setter Property="FontSize" Value="20" />
    </Style>

    <Style x:Key="Text.Body" TargetType="TextBlock" BasedOn="{StaticResource Text.Base}">
        <Setter Property="FontSize" Value="16" />
        <Setter Property="TextWrapping" Value="Wrap" />
    </Style>

    <Style x:Key="Text.Post.Body" TargetType="TextBlock" BasedOn="{StaticResource Text.Body}">
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Margin" Value="8" />
    </Style>

    <Style x:Key="Text.Post.Author" TargetType="TextBlock" BasedOn="{StaticResource Text.Base}">
        <Setter Property="FontStyle" Value="Italic" />
        <Setter Property="Foreground" Value="DarkSlateGray" />
        <Setter Property="FontSize" Value="10" />
    </Style>

    <Style x:Key="Text.Comment.Author" TargetType="TextBlock" BasedOn="{StaticResource Text.Base}">
        <Setter Property="FontStyle" Value="Italic" />
        <Setter Property="Foreground" Value="DarkSlateGray" />
        <Setter Property="FontSize" Value="10" />
    </Style>

    <Style x:Key="Text.Comment.Body" TargetType="TextBlock" BasedOn="{StaticResource Text.Body}">
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Margin" Value="8" />
    </Style>

    <!-- textbox styles -->

    <Style x:Key="App.TextBox" TargetType="TextBox">
        <Setter Property="FontFamily" Value="Segoe UI Light" />
        <Setter Property="FontSize" Value="18" />
        <Setter Property="BorderBrush" Value="SteelBlue" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Background" Value="{x:Null}" />
    </Style>

    <!-- Post summary styles -->
    <Style x:Key="Post.Border" TargetType="Border">
        <Setter Property="Background" Value="LightSteelBlue" />
    </Style>

    <Style x:Key="Post.Photo.Border" TargetType="Border">
        <Setter Property="Background" Value="SkyBlue" />
        <Setter Property="Height" Value="50" />
        <Setter Property="Width" Value="50" />
        <Setter Property="Margin" Value="4" />
        <Setter Property="CornerRadius" Value="4" />
    </Style>

    <!-- Buttons -->
    <Style x:Key="App.Button" TargetType="Button">
        <Setter Property="Foreground" Value="AliceBlue" />
        <Setter Property="Background" Value="SteelBlue" />
        <Setter Property="FontFamily" Value="Segoe UI Light" />
        <Setter Property="FontSize" Value="16" />
        <Setter Property="Margin" Value="8" />
        <Setter Property="Padding" Value="8" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid>
                        <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="4" />
                        <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Margin="{TemplateBinding Padding}" />
                    </Grid>
                <ControlTemplate.Triggers>
                    <Trigger Property="IsPressed" Value="True">
                        <Setter Property="Background" Value="Black" />
                    </Trigger>
                </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="Error.Button" TargetType="Button" BasedOn="{StaticResource App.Button}">
        <Setter Property="Foreground" Value="AntiqueWhite" />
        <Setter Property="Background" Value="#44FFFFFF" />
    </Style>

